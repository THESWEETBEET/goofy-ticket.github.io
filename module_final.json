  "search": {
    "type": "json",
    "description": "Primary: read from repo-hosted content.json. Fallback: basic HTML meta scraping.",
    "raw_json": {
      "url": "https://raw.githubusercontent.com/THESWEETBEET/goofy-ticket.github.io/main/content.json",
      "results": {
        "title": "$json:title",
        "image": "$json:image",
        "url": "$json:detail_url",
        "description": "$json:description"
      }
    },
    "inline_fallback": {
      "results": [
        {
          "title": "Goofy Ticket \u2014 Demo Video 1",
          "image": "https://via.placeholder.com/640x360.png?text=Demo+1",
